{
  "name": "pos-ui-react",
  "version": "1.0.0-beta.6",
  "description": "A React component library with theme support",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./components/button": {
      "types": "./dist/components/button.d.ts",
      "import": "./dist/components/button.es.js",
      "require": "./dist/components/button.cjs.js"
    },
    "./components/themeToggle": {
      "types": "./dist/components/themeToggle.d.ts",
      "import": "./dist/components/themeToggle.es.js",
      "require": "./dist/components/themeToggle.cjs.js"
    },
    "./components/icon": {
      "types": "./dist/components/icon.d.ts",
      "import": "./dist/components/icon.es.js",
      "require": "./dist/components/icon.cjs.js"
    },
    "./components/toast": {
      "types": "./dist/components/toast.d.ts",
      "import": "./dist/components/toast.es.js",
      "require": "./dist/components/toast.cjs.js"
    },
    "./blocks/functionCard": {
      "types": "./dist/blocks/functionCard.d.ts",
      "import": "./dist/blocks/functionCard.es.js",
      "require": "./dist/blocks/functionCard.cjs.js"
    },
    "./contexts/theme": {
      "types": "./dist/contexts/theme.d.ts",
      "import": "./dist/contexts/theme.es.js",
      "require": "./dist/contexts/theme.cjs.js"
    },
    "./hooks/useTheme": {
      "types": "./dist/hooks/useTheme.d.ts",
      "import": "./dist/hooks/useTheme.es.js",
      "require": "./dist/hooks/useTheme.cjs.js"
    },
    "./style.css": "./dist/pos-ui-react.css",
    "./dist/pos-ui-react.css": "./dist/pos-ui-react.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "vite build --mode library",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:preview": "dumi preview",
    "docs:deploy": "pnpm docs:build && vercel --prod",
    "prepublishOnly": "pnpm run build:lib"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/builder-vite": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "babel-plugin-import": "^1.13.8",
    "dumi": "^2.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "playwright": "^1.56.0",
    "sass": "^1.93.2",
    "storybook": "^9.1.10",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vitest": "^3.2.4"
  }
}
